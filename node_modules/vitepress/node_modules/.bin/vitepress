#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Code/blog/my-site-blog/node_modules/.pnpm/vitepress@1.5.0_@algolia+client-search@5.17_7nvpkzdeyxxdq5t4umlufyiuaa/node_modules/vitepress/bin/node_modules:/mnt/d/Code/blog/my-site-blog/node_modules/.pnpm/vitepress@1.5.0_@algolia+client-search@5.17_7nvpkzdeyxxdq5t4umlufyiuaa/node_modules/vitepress/node_modules:/mnt/d/Code/blog/my-site-blog/node_modules/.pnpm/vitepress@1.5.0_@algolia+client-search@5.17_7nvpkzdeyxxdq5t4umlufyiuaa/node_modules:/mnt/d/Code/blog/my-site-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Code/blog/my-site-blog/node_modules/.pnpm/vitepress@1.5.0_@algolia+client-search@5.17_7nvpkzdeyxxdq5t4umlufyiuaa/node_modules/vitepress/bin/node_modules:/mnt/d/Code/blog/my-site-blog/node_modules/.pnpm/vitepress@1.5.0_@algolia+client-search@5.17_7nvpkzdeyxxdq5t4umlufyiuaa/node_modules/vitepress/node_modules:/mnt/d/Code/blog/my-site-blog/node_modules/.pnpm/vitepress@1.5.0_@algolia+client-search@5.17_7nvpkzdeyxxdq5t4umlufyiuaa/node_modules:/mnt/d/Code/blog/my-site-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress.js" "$@"
fi
